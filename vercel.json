{
  "version": 2,
  "builds": [
    {
      "src": "./src/index.ts",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "bun build src/index.ts --compile --outfile dist/index.js",
        "installCommand": "bun install"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/index.ts"
    }
  ]
}
